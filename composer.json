{
    "name": "browner12/larasearch",
    "type": "library",
    "description": "Search package for Laravel",
    "keywords": [
        "browner12",
        "larasearch"
    ],
    "homepage": "https://github.com/browner12/larasearch",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Brown",
            "email": "browner12@gmail.com",
            "homepage": "https://github.com/browner12",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "illuminate/console": "~5.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "elasticsearch/elasticsearch": "~5.0",
        "phpunit/phpunit": "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "browner12\\larasearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "browner12\\larasearch\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
